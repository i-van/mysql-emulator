{
  "name": "mysql-emulator",
  "version": "0.1.0",
  "description": "MySQL emulator",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:db": "jest --setupFilesAfterEnv ./__tests__/tear-up",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "lint": "eslint '{src,examples,__tests__}/**/*.ts' --fix"
  },
  "author": "Ivan Nosov",
  "repository": "https://github.com/i-van/mysql-emulator",
  "license": "MIT",
  "keywords": [
    "mysql",
    "mysql-mock",
    "mock",
    "mysql-emulator",
    "emulator",
    "db",
    "database",
    "typeorm",
    "sequelize",
    "unit test"
  ],
  "dependencies": {
    "node-sql-parser": "4.9.0"
  },
  "peerDependencies": {
    "sequelize": ">=6.0.0",
    "typeorm": ">=0.3.0"
  },
  "peerDependenciesMeta": {
    "sequelize": {
      "optional": true
    },
    "typeorm": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/node": "20.11.16",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "dotenv": "16.4.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "mysql2": "3.9.1",
    "rimraf": "5.0.5",
    "sequelize": "6.36.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typeorm": "0.3.20",
    "typescript": "5.3.3"
  }
}
