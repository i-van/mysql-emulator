    "dotenv": "16.3.1",
    "eslint": "8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "mysql2": "3.4.3",
    "rimraf": "5.0.1",
    "sequelize": "6.32.1",
    "ts-jest": "29.1.1",
    "ts-node": "^10.7.0",
    "typeorm": "0.3.17",
    "typescript": "5.1.6"
